{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "allowJs": true,
        "checkJs": true,
        "outDir": "./dist",
        "strict": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
